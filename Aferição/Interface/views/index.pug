extends layout
block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 EMDApp 
      button.w3-button.w3-left
        a(href="/emd/insert") [New EMD]
    .w3-card-4
      .w3-container
        table.w3-table-all
          tr
            th Data 
            th Nome 
            th Resultado 

          each item in list
              tr 
                if item.dataEMD && item.nome && item.resultado
                  td #{item.dataEMD}
                  td #{item.nome.primeiro + ' ' + item.nome.Ãºltimo}
                  td #{item.resultado}
                  td 
                    a(href="/emd/" + item._id) EMD


              
    footer.w3-container.w3-teal
      h5 Generated by EMDApp

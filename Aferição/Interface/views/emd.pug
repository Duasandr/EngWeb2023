extends layout
block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 EMDApp
    .w3-card-4
      .w3-container
        table.w3-table-all.w3-hoverable
            each value, key in emd
                tr 
                    td #{key} 
                    td
                       if value instanceof Object 
                            each v, k in value
                                span.w3-tag.w3-teal.w3-round.w3-margin-right #{k} 
                                span.w3-margin-right #{v}
                       else
                            span #{value}
                              
    footer.w3-container.w3-teal
      h5 Generated by EMDApp
      a.w3-button(href="/") [Back to home]